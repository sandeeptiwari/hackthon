<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
   >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">


        <TextView
            android:id="@+id/textView2"
            style="@style/card_field_style"
            android:text="Request Number"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            style="@style/card_field_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Account Holder"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

        <TextView
            android:id="@+id/textView4"
            style="@style/card_field_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bank"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView5"
            style="@style/card_field_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Claim Number"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView6"
            style="@style/card_field_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Account Number"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView7"
            style="@style/card_field_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="IFSC Code"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/text_view_patient_name"
            style="@style/card_answer_text"
            android:text="MA1287923"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_view_gender"
            style="@style/card_answer_text"
            android:text="Wellcare Health Services"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/text_view_patient_name" />

        <TextView
            android:id="@+id/text_view_ailment"
            style="@style/card_answer_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="HDFC"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/text_view_gender" />

        <TextView
            android:id="@+id/text_view_submission_date"
            style="@style/card_answer_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2917813"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/text_view_ailment" />

        <TextView
            android:id="@+id/text_view_discharge_date"
            style="@style/card_answer_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8237498267398"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/text_view_submission_date" />

        <TextView
            android:id="@+id/text_view_hospital"
            style="@style/card_answer_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="HDFC8927348"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/text_view_discharge_date" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>